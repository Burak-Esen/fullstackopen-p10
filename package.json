{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "test": "jest",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint ./src/**/*.{js,jsx} App.js --no-error-on-unmatched-pattern"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*|react-router-native)"
    ]
  },
  "dependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "@react-native-community/async-storage": "~1.11.0",
    "apollo-boost": "^0.4.9",
    "dotenv": "^8.2.0",
    "expo": "~38.0.8",
    "expo-linear-gradient": "~8.2.1",
    "expo-linking": "^1.0.7",
    "expo-status-bar": "^1.0.2",
    "formik": "^2.2.6",
    "graphql": "^14.7.0",
    "react": "~16.11.0",
    "react-dom": "~16.11.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-38.0.2.tar.gz",
    "react-native-paper": "^4.8.1",
    "react-native-vector-icons": "^8.1.0",
    "react-native-web": "~0.11.7",
    "react-router-native": "^5.2.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@expo/webpack-config": "^0.12.68",
    "@testing-library/jest-native": "^4.0.1",
    "@testing-library/react-native": "^7.2.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-expo": "~8.1.0",
    "eslint": "^7.25.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-react": "^7.23.2",
    "jest": "^26.6.3",
    "jest-expo": "^41.0.0"
  },
  "private": true
}
